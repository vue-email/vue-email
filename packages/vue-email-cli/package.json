    "@commander-js/extra-typings": "9.4.1",
    "@manypkg/find-root": "2.2.1",
    "@octokit/rest": "19.0.7",
    "chokidar": "3.5.3",
    "commander": "9.4.1",
    "detect-package-manager": "2.0.1",
    "esbuild": "0.16.4",
    "fs-extra": "11.1.1",
    "glob": "8.0.3",
    "log-symbols": "4.1.0",
    "normalize-path": "3.0.0",
    "ora": "5.4.1",
    "read-pkg": "5.2.0",
    "shelljs": "0.8.5",
    "tree-node-cli": "1.6.0"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.1",
    "@types/node": "18.11.9",
    "@types/normalize-path": "3.0.0",
    "@types/shelljs": "0.8.12",
    "prettier": "3.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  }
}
