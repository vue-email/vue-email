{
  "name": "vue-email-cli",
  "version": "1.9.4",
  "description": "A live preview of your emails right in your browser.",
  "bin": {
    "vue-email": "./dist/source/index.js"
  },
  "types": "./dist/source/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format:check": "prettier --check \"**/*.{js,ts,tsx,json}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,json}\"",
    "lint": "eslint",
    "clean": "rm -rf dist"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
